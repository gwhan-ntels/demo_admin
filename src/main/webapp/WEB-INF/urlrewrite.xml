<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/favicon.ico</from>
		<to>/images/favicon.ico</to>
	</rule>
	<rule>
		<from>/help/**</from>
		<to>/help/$1</to>
	</rule>
	<rule>
		<from>/audio/**</from>
		<to>/audio/$1</to>
	</rule>
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/scripts/**</from>
		<to>/scripts/$1</to>
	</rule>
	<rule>
		<from>/styles/**</from>
		<to>/styles/$1</to>
	</rule>
    <rule>
        <from>/interface/*.req</from>
        <to>/interface/$1</to>
    </rule>
	<rule>
		<from>/**</from>
		<to>/adminapp/$1</to>
	</rule>
	<outbound-rule>
		<from>/adminapp/**</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>
