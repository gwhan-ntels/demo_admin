/* window popup */
body.opopup{min-width:100% !important;}

/* label */
input[type='checkbox'] + label,
input[type='radio'] + label,
input[type='checkbox']:disabled + label,
input[type='radio']:disabled + label{color:#535353;}
input[type='checkbox']:checked + label,
input[type='radio']:checked + label{color:#222222;}

/* checkbox */ /*다시확인할것*/
input[type='checkbox']{position:relative; padding:0;}
input[type='checkbox']::after{position:absolute; left:-2px; top:-2px; content:''; cursor:pointer; display:inline-block; width:18px; height:18px; background:url('../../images/icon/checkbox_icon.png') no-repeat 0 0; background-size:100%;}
input[type='checkbox']:checked::after{background-position:0 -18px;}
input[type='checkbox']:disabled::after{background-position:0 -36px;}

/* radioBtn*/
input[type='radio']{position:relative; padding:0;}
input[type='radio']::after{position:absolute; left:-2px; top:-2px; content:''; cursor:pointer; display:inline-block; width:19px; height:19px; background:url('../../images/icon/radio_icon.png')no-repeat 0 0; background-size:100%;}
input[type='radio']:checked::after{background-position:0 -19px;}
input[type='radio']:disabled::after{background-position:0 -38px;}


