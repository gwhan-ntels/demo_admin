.error_warning .image{width:60px; height:60px; background:url('../../images/common/error.png') no-repeat 0 0; background-size:100%; display:inline-block; vertical-align:middle; margin-left:39px}
.error_warning p{padding:6px 16px; display:inline-block; vertical-align:middle;}
.error_warning .image:after,
.error_warning p:after{display:block; clear:both; content:'';}
.error_warning p > span{display:block;}
.error_warning p,
.error_warning span{font-size:16px; line-height:24px; font-weight:600;}
.error_infoArea{width:516px; border:2px solid #d7d7d7; margin: 40px 0px 24px 0px; border-radius:5px;}
.error_infoArea > p{color:#2c7ed9; text-align:center; padding:30px; 10px; font-size:16px; font-weight:600;}
