/* 테이블리스트 */
.writelist { border-top:2px  solid #a4dd7f;border-bottom:1px  solid #dcdcdc;;width:100%;table-layout: fixed;}
.writelist tr:hover{background-color:#f5ffe1;color:#baf04d;}
.writelist th{font-weight: normal;font-size:11px;background-color:#f6f6f6; padding:3px 5px ; border-bottom:1px solid #dcdcdc; border-right:1px solid #dcdcdc; color:#5aa829;text-align:center;}
.writelist td{position: relative;font-size:11px;color:#666; padding:3px 10px ;  border-bottom:1px solid #dcdcdc; border-right:1px solid #dcdcdc; text-align:center;}
.writelist .lef{ text-align:left;white-space: nowrap; text-overflow: ellipsis;vertical-align: middle; overflow: hidden;}
.writelist .rig{ text-align:left;white-space: nowrap; text-overflow: ellipsis;vertical-align: middle; overflow: hidden;}
.writelist thead tr th[colspan] {border-bottom:1px dotted #dcdcdc;}
.writelist thead tr th[rowspan]:first-child {border-right:1px solid #dcdcdc;}
.writelist tbody tr th[colspan] {border-bottom:1px dotted #dcdcdc;}
.writelist tbody tr th[rowspan]:first-child {border-right:1px solid #dcdcdc;}
.writelist thead tr td[rowspan] {border-right:1px solid #dcdcdc;}
.writelist tbody tr td[rowspan] {border-right:1px solid #dcdcdc;}
.writelist thead tr th:last-child {border-right:0;}
.writelist tbody tr td:last-child {border-right:0;}
.writelist thead tr td:last-child {border-right:0;}


/* 테이블뷰 */
.writeview { border-top:2px  solid #a4dd7f;border-bottom:1px  solid #dcdcdc;width:100%;}
.writeview th{position: relative;letter-spacing: -0.1em;font-size:11px;font-weight: normal;background-color:#f6f6f6;padding:3px 11px 3px 5px; border-bottom:1px solid #dcdcdc; border-right:1px solid #dcdcdc; color:#5aa829;text-align:left;text-overflow:ellipsis; white-space:nowrap; overflow:hidden}
.writeview td{position: relative;font-size:11px;color:#000; padding:3px 5px ; font-size:11px; border-bottom:1px solid #dcdcdc; border-right:1px solid #dcdcdc; text-align:left;}
.writeview .text{color:#ff0000; padding: 5px;text-align:left;}
.writeview thead tr th[rowspan]:first-child {border-right:1px solid #dcdcdc;}
.writeview tbody tr td[rowspan] {border-right:1px solid #dcdcdc;}
.writeview tbody tr td:last-child {border-right:0;}
.writeview thead tr td:last-child {border-right:0;}
.writeview  label{background-color:#fff;}


