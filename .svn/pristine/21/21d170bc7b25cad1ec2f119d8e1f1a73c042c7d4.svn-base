		        '0' AS CHK,
                TDRI.MNO_RATE_ITM_CD               
		FROM    TPMPD_D_RATE_ITM   TDRI,
		        TPMPD_SVC_RATE_ITM_TYP TSRIT
		WHERE   TDRI.SVC_RATE_ITM_TYP_CD = TSRIT.SVC_RATE_ITM_TYP_CD
