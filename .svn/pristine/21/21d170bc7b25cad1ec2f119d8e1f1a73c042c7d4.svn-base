                        ,TPMPD_PROD   TX
					WHERE TDPR.INACT_DT >= #{productDevMgt.currentDay}
					  AND TDPR.MSTR_FL = '1'
					  AND TDPR.REL_PROD_CD = #{productDevMgt.prodCd}
                      AND TDPR.REL_PROD_CD = TP.PROD_CD
