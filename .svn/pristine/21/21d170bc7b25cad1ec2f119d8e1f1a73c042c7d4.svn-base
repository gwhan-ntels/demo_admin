		           AND B.COMMON_CD = C.COMMON_CD
		           WHERE  B.COMMON_GRP  = 'PP00031'
		           AND    B.COMMON_CD   = TDRI.CHRG_CTGRY
		        ) CHRG_CTGRY_NM,
