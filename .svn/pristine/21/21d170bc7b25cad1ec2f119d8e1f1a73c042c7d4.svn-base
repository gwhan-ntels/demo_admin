		AND     TSRIT.INACT_DT  >= #{productDevMgt.currentDay}
		AND     TDRI.PROD_CD      = #{productDevMgt.prodCd}
		AND     TDRI.SVC_CD       = #{productDevMgt.svcCd}
		ORDER BY TDRI.DISP_PRI_NO
