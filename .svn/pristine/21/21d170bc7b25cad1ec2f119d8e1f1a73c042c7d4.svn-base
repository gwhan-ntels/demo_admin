				<when test="'${dbms.kind}' == 'ORACLE'">                       
		      		CHG_DATE = SYSDATE 
				</when>                          
				<when test="'${dbms.kind}' == 'MYSQL'">                         
		        	CHG_DATE = NOW()
				</when>
			</choose>
