		SELECT #{productDevMgt.rateItmCd} RATE_ITM_CD,
		       ( CONCAT(CONCAT((SELECT PROD_NM
		          FROM   TPMPD_D_PROD
		          WHERE  PROD_CD = #{productDevMgt.prodCd}),' '), V_1.SVC_RATE_ITM_TYP_NM)
