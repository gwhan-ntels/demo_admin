					  (
					    SELECT TP.PROD_NM
					    FROM TPMPD_PROD TP
					    WHERE TDPR.REL_PROD_CD = TP.PROD_CD
					      AND TP.MSTR_FL = '1'
					      AND TP.INACT_DT >= #{productDevMgt.currentDay}) REL_PROD_NM ,
					      TDPR.INACT_DT,
                          TDPR.DEF_YN
                    ,TP.SO_ID
                    ,(select SO_NM 
                         from   TSYCO_SO_MAST
                         where  SO_ID = TP.SO_ID
                         and    USE_YN = 'Y') AS SO_NM
					FROM TPMPD_PROD_REL TDPR
