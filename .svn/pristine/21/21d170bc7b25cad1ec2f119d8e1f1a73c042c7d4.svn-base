                   LEFT OUTER JOIN TSYCO_CODE_DETAIL_NAME C
		           ON #{productDevMgt.lngTyp} = C.LNG_TYP
		           AND B.COMMON_GRP = C.COMMON_GRP
		           AND B.COMMON_CD = C.COMMON_CD
		           WHERE  B.COMMON_GRP  = 'PP00034'
		           AND    B.COMMON_CD   = TDRI.RATE_LOCAT
