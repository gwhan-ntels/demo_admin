package com.ntels.ccbs.distribute.domain.logistics.voucherMgt;

import java.io.Serializable;

import com.ntels.ccbs.system.domain.common.service.CommonVO;
import com.ntels.ccbs.system.domain.common.service.Paging;

public class VoucherItemVO extends Paging implements Serializable, CommonVO {

	private static final long serialVersionUID = 1L;

	private String soId;
	private String soNm;
	private String prodCd;
	private String prodNm;
	private Double prodAmt;
	private String qosAmt;
	private String dhType;
	private String effPeriod;
	private String pcrfYn;
	private String pdvcYn;
	private String todayDt;

	public String getSoId() {
		return soId;
	}

	public void setSoId(String soId) {
		this.soId = soId;
	}

	public String getSoNm() {
		return soNm;
	}

	public void setSoNm(String soNm) {
		this.soNm = soNm;
	}

	public String getProdCd() {
		return prodCd;
	}

	public void setProdCd(String prodCd) {
		this.prodCd = prodCd;
